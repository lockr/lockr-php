{
    "name": "lockr/lockr",
    "description": "Client for use with Lockr off-site key management service.",
    "type": "library",
    "authors": [
        {
            "name": "Tynor Fujimoto",
            "email": "tynor@lockr.io"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^6.3.0 || ^7.4.0",
        "guzzlehttp/psr7": "^1.4.2 || ^2.2.0",
        "indigophp/hash-compat": "^1.1.0",
        "psr/http-message": "^1.0.1 || ^2.0",
        "symfony/yaml": "~3.4.5 || ^4.0.0 || ^5.0.0 || ^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "Lockr\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3.5 || ^9.5.0",
        "squizlabs/php_codesniffer": "^3.3.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Lockr\\Tests\\": "tests/"
        }
    }
}
